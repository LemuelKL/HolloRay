-sequenceDone
---
seq(__done, _C) -> _C

-sequence 
_C1 -> _C1P
---
seq(_C1, _C2) -> seq(_C1P, _C2)

-ifTrue 
---
if(True, _C1, _C2) -> _C1

-ifFalse 
---
if(False, _C1, _C2) -> _C2

-ifResolve
_E ->_EP
---
if(_E,_C1,_C2) -> if(_EP, _C1, _C2)

-while
---
while(_E, _C) -> if(_E, seq(_C, while(_E,_C)), __done)

!try while(False, __done)

//!try while(True, __done)
